{
  "name": "80sBaby",
  "version": "1.0.0",
  "description": "An 80s inpired retro react component library made with typescript",
  "main": "core/index.js",
  "types": "core/index.d.ts",
  "repository": "https://github.com/Harrison1/80sBaby",
  "author": "Harrison McGuire",
  "license": "MIT",
  "keywords": [
    "80s",
    "Baby",
    "Retro",
    "React",
    "Component",
    "Library"
  ],
  "files": [
    "core/**/*"
  ],
  "dependencies": {
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@storybook/addon-a11y": "5.3.12",
    "@storybook/addon-actions": "5.3.12",
    "@storybook/addon-knobs": "5.3.12",
    "@storybook/addon-links": "5.3.12",
    "@storybook/addon-viewport": "5.3.12",
    "@storybook/addons": "5.3.12",
    "@storybook/react": "5.3.12",
    "@types/jest": "24.9.1",
    "@types/react-test-renderer": "16.9.2",
    "@types/storybook__addon-a11y": "5.1.2",
    "@types/storybook__addon-knobs": "5.2.1",
    "@types/storybook__react": "4.0.2",
    "@types/styled-components": "4.4.2",
    "awesome-typescript-loader": "5.2.1",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "react-docgen-typescript-loader": "3.6.0",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-test-renderer": "16.12.0",
    "source-map-loader": "0.2.4",
    "ts-jest": "24.3.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.5",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.10"
  },
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  }
}
